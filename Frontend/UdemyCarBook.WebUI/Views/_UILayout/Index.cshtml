@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_HeadLayoutComponent")
<body>

    @await Component.InvokeAsync("_NavbarLayoutComponent")
    <!-- END nav -->
    @await Component.InvokeAsync("_FeatureLayoutComponent")

    @RenderBody()

    @await Component.InvokeAsync("_FooterLayoutComponent")

    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>

    @await Component.InvokeAsync("_ScriptLayoutComponent")
</body>
</html>